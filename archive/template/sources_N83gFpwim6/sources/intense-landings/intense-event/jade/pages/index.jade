		.rd-navbar-wrap
			nav.rd-navbar.rd-navbar-default(data-sm-layout='rd-navbar-static',
			data-sm-device-layout='rd-navbar-static',
			data-sm-auto-height='true',
			data-sm-stick-up='true')
				.rd-navbar-inner
					// RD Navbar Panel
					.rd-navbar-panel
						// RD Navbar Toggle
						.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar, .rd-navbar-list')
							span
						// Navbar Brand
						.rd-navbar-brand
							a.rd-navbar-brand-static(href='../../index.html')
								img(width='187', height='32', src='images/logo-dark-187x32.png', alt='')
							a.rd-navbar-brand-fixed(href='../../index.html')
								img(width='187', height='32', src='images/logo-dark-187x32.png', alt='')
