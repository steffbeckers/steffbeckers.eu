extends  ../_skeleton

//-Includes
include ../elements/_widget-post
include ../elements/_widget-form-subscribe
include ../elements/_widget-list-social
include ../elements/_ui-testimonials-slider-2
include ../elements/_ui-box-planning-type-3

//-Title
block title
  title Intense Travel Agency

//- Page
block page

  // Page Head
  header.page-head.slider-menu-position
    // RD Navbar Transparent
    .rd-navbar-wrap
      nav.rd-navbar.rd-navbar-default(data-sm-layout='rd-navbar-static',
                                      data-sm-device-layout='rd-navbar-static',
                                      data-sm-auto-height='true',
                                      data-sm-stick-up='true')
        .rd-navbar-inner
          // RD Navbar Panel
          .rd-navbar-panel
            // RD Navbar Toggle
            .rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar, .rd-navbar-nav-wrap')
              span
            // Navbar Brand
            .rd-navbar-brand
              a(href='index.html')
                img(width='220', height='32', src='images/logo-light-220x32.png', alt='')
          .rd-navbar-menu-wrap
            .rd-navbar-nav-wrap
              .rd-navbar-mobile-scroll
                // RD Navbar Nav
                ul.rd-navbar-nav
                  li
                    span.mdi.mdi-phone.text-middle
                    a.reveal-inline-block.text-middle.inset-left-10.text-middle(href='callto:+1234567890') +1 234 567 890
                  li
                    span.mdi.mdi-email-outline.text-middle
                    a.reveal-inline-block.text-middle.inset-left-10.text-middle(href='mailto:mail@demolink.org') mail@demolink.org
                  li
                    a.btn.btn-primary.btn-icon.btn-icon-left.btn-sm.btn-ellipse(href="//www.templatemonster.com/intense-multipurpose-html-template.html?utm_source=livedemo&utm_medium=themebutton&utm_campaign=intense")
                      span.icon.mdi.mdi-telegram
                      | BOOK NOW

  // Page Contents
  main.page-content
    section
      // Swiper
      .swiper-container.swiper-slider(data-height='100vh', data-min-height='400px')
        .swiper-wrapper
          .swiper-slide.swiper-slide-overlay-disable(data-slide-bg='images/slide-01-1920x1000.jpg')
            .swiper-slide-caption
              .shell
                .range.range-xs-center.range-lg-left
                  .cell-xs-10.cell-lg-6.text-lg-left
                    h1(data-caption-animate="fadeInUp" data-caption-delay="400") Experience The Best Trip Ever
                    p(data-caption-animate="fadeInUp" data-caption-delay="500") We are committed to offering travel services of the highest quality, combining our energy and enthusiasm with years of experience.
                    .offset-top-30.offset-lg-top-60(data-caption-animate="fadeInUp" data-caption-delay="600")
                      a.btn.btn-primary.btn-icon.btn-icon-left.btn-sm.btn-ellipse(href="#")
                        span.icon.mdi.mdi-telegram
                        | BOOK a TOUR
          .swiper-slide(data-slide-bg='images/slide-02-1920x1000.jpg')
            .swiper-slide-caption
              .shell
                .range.range-xs-center.range-lg-left
                  .cell-xs-10.cell-lg-6.text-lg-left
                    h1(data-caption-animate="fadeInUp" data-caption-delay="400") Let`s Make Your Best Trip Ever
                    p(data-caption-animate="fadeInUp" data-caption-delay="500") We are committed to offering travel services of the highest quality, combining our energy and enthusiasm, with our years of experience.
                    .offset-top-30.offset-lg-top-60(data-caption-animate="fadeInUp" data-caption-delay="600")
