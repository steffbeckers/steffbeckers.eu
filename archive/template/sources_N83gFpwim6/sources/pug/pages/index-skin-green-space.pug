- var fonts = "//fonts.googleapis.com/css?family=Crimson+Text:700italic,400,400italic,700%7CMontserrat:400,700"
- var html_class = "wide wow-animation smoothscroll scrollTo"

//- Includes
include ../_config
include ../_mixins
include ../mixins/_gallery-photoswipe
include ../elements/_ui-page-loader-variant-1
include ../elements/_ui-rd-navbar-default-transparent
include ../elements/_widget-form-contact
include ../elements/_widget-post
include ../elements/_widget-list-social
include ../elements/_ui-rd-parallax
