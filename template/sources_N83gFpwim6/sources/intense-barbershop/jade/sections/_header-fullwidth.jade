header.page-header
  //- RD Navbar
  .rd-navbar-wrap
    nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-sm-device-layout='rd-navbar-fixed', data-md-layout='rd-navbar-fullwidth', data-md-device-layout='rd-navbar-fixed', data-lg-device-layout='rd-navbar-fixed', data-lg-layout='rd-navbar-fullwidth', data-stick-up-clone='true', data-md-stick-up-offset='140px', data-lg-stick-up-offset='140px')
      .rd-navbar-inner
        //- RD Navbar Panel
        .rd-navbar-panel
          //- RD Navbar Toggle
          button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap')
            span
          //- RD Navbar Brand
          .rd-navbar-brand
            a.brand-name(href='index.html')
              .brand-mobile
                +png('logo-default-199x36')
              .brand-desktop
                +png('logo-109x125')

        .rd-navbar-nav-wrap
          .rd-navbar-nav-inner
            include ../includes/_nav